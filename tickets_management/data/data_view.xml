<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <data noupdate="1">

        <record id="ir_cron_auto_close_tickets" model="ir.cron">
            <field name="name">Auto Close Tickets</field>
            <field name="model_id" ref="model_ticket_management"/>
            <field name="state">code</field>
            <field name="code">model.auto_close_tickets()</field>
            <field name="interval_number">2</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
        </record>

        <record id="ir_cron_auto_change_sla_status" model="ir.cron">
            <field name="name">Auto Update SLA Status</field>
            <field name="model_id" ref="model_ticket_management"/>
            <field name="state">code</field>
            <field name="code">model.auto_change_sla_respond_status()</field>
            <field name="interval_number">4</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
        </record>

        <record id="ir_cron_auto_change_sla_status2" model="ir.cron">
            <field name="name">Auto Update SLA Status2</field>
            <field name="model_id" ref="model_ticket_management"/>
            <field name="state">code</field>
            <field name="code">model.auto_change_sla_resolve_status()</field>
            <field name="interval_number">4</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
        </record>

        <record id="sequence_ticket_seq" model="ir.sequence">
            <field name="name">Ticket Sequence</field>
            <field name="code">ticket.management</field>
            <field name="prefix">2081/82/TIK</field>
            <field name="padding">5</field>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>

    </data>
</odoo>
