<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_stock_tree_inherited" model="ir.ui.view">
        <field name="name">stock.picking.tree.inherited</field>
        <field name="model">stocking.picking</field>
        <field name="inherited_id" ref="stock.vpicktree"/>
        <field name="arch" type="xml">
           <xpath expr="//field[@name='user_id']" position="after">
                <field name="challan_no" string="Invoice No/Challan" optional="show"/>
           </xpath>
        </field>
    </record>
</odoo>